{
    "name": "driskell/magento-ibuprofen",
    "type": "magento-module",
    "license": "Apache-2.0",
    "description": "Medicine for the migraines of Magento 1's JS/CSS concatenation with sourcemap and minification moisterizer.",
    "authors": [
        {
            "name": "Jason Woods",
            "email": "devel@jasonwoods.me.uk"
        }
    ],
    "homepage": "https://github.com/driskell/magento-ibuprofen",
    "require": {
        "magento-hackathon/magento-composer-installer": "*",
        "matthiasmullie/minify": "^1.3",
        "eloquent/composer-npm-bridge": "4.1.0"
    },
    "require-dev": {
        "composer/composer": "dev-master"
    },
    "extra": {
        "magento-root-dir": "root",
        "npm-bridge": {
            "optional": true
        },
        "map": [
            ["app/code/community/Driskell/Ibuprofen", "app/code/community/Driskell/Ibuprofen"],
            ["app/etc/modules/Driskell_Ibuprofen.xml", "app/etc/modules/Driskell_Ibuprofen.xml"],
            ["node_modules", "app/code/community/Driskell/Ibuprofen/node_modules"]
        ]
    }
}
